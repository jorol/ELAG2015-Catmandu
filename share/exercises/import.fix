# Import fix for perl_books.json
split_field(author, '; ')
replace_all(author.*, '\s\d+-(\d+)?$', '')
split_field(editor, '; ')
replace_all(editor.*, '\s\d+-(\d+)?$', '')
split_field(keyword, '; ')
split_field(isbn, '; ')
split_field(publisher, ' : ')
copy_field(publisher.0,publisherPlace)
copy_field(publisher.1,publisherName)
lookup('language', 'dict_languages.csv');
remove_field(publisher)